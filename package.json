{
  "name": "recast.ly",
  "version": "1.1.0",
  "description": "Making YouTube with React and ES6",
  "author": "Hack Reactor <curriculum.team@hackreactor.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start":  "npm run babel & live-server",
    "babel": "babel . --out-dir compiled --presets=es2015,react --ignore=node_modules,compiled --source-maps inline --watch",
    "test": "live-server --open=./SpecRunner.html --port=8081",
    "postinstall": "curl -s https://raw.githubusercontent.com/hackreactor-labs/pomander/master/bin/install | bash",
    "compile": "babel-compile -p es2015 src:lib test:.test",
    "pretest": "npm run compile"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "jquery": "^2.2.1",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-compile": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "2.3.3",
    "sinon": "^1.17.3"
  }
}
